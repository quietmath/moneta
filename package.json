{
  "name": "@quietmath/moneta",
  "version": "1.0.0",
  "description": "A Node.JS JSON store for lightweight database storage need.",
  "main": "./dist/index.js",
  "private" :false,
  "nyc": {
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/example/**/*.js",
      "**/example/**/*.ts",
      "**/examples/**/*.js",
      "**/*.d.ts",
      "**/*.test.js",
      "**/*.js.map",
      "**/node_modules/**",
      "./node_modules/**"
    ],
    "reporter": [
      "text",
      "json-summary"
    ],
    "report-dir": "./.nyc_output/coverage"
  },
  "license": "MIT",
  "author": "Michael Szul <michael@szul.us>",
  "contributors": [
    "Michael Szul <michael@szul.us>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quietmath/moneta.git",
  },
  "bugs": {
    "url": "https://github.com/quietmath/moneta/issues"
  },
  "homepage": "https://github.com/quietmath/moneta#readme",
  "scripts": {
    "build": "tsc -p ./tsconfig",
    "test": "nyc mocha ./**/test/*.test.js --ignore ./**/node_modules/**",
    "eslint": "eslint ./src/*.ts ./src/**/*.ts",
    "eslint-fix": "eslint ./src/*.ts ./src/**/*.ts --fix"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "lodash": "^4.17.20",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "eslint": "^7.8.0",
    "exorcist": "^1.0.1",
    "madge": "^3.9.0",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1",
    "rewire": "^5.0.0",
    "typescript": "^4.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
